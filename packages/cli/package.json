{
  "name": "@beemo/cli",
  "version": "0.16.0",
  "description": "Beemo CLI.",
  "keywords": [
    "beemo",
    "cli",
    "command"
  ],
  "bin": {
    "beemo": "./bin/beemo.js"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "build": "beemo typescript",
    "type": "beemo typescript --noEmit"
  },
  "repository": "https://github.com/milesj/beemo/tree/master/packages/cli",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@oclif/command": "^1.4.17",
    "@oclif/config": "^1.6.15",
    "@oclif/plugin-help": "^1.2.5",
    "@oclif/plugin-not-found": "^1.0.7",
    "@oclif/plugin-warn-if-update-available": "^1.3.6",
    "chalk": "^2.4.1",
    "semver": "^5.5.0",
    "yargs": "^11.0.0"
  },
  "peerDependencies": {
    "@beemo/core": "^0.16.0"
  },
  "devDependencies": {
    "@types/semver": "^5.5.0",
    "@types/yargs": "^11.0.0"
  },
  "oclif": {
    "bin": "beemo",
    "commands": "./lib/commands",
    "hooks": {
      "init": "./lib/hooks/init"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  }
}
